export PYTHONPATH=$SPARK_HOME/python/:$SPARK_HOME/python/build:$SPARK_HOME/python/lib/py4j-0.10.3-src.zip:$PYTHONPATH

# java_home
export JAVA_HOME=$(/usr/libexec/java_home)
